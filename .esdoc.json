{
  "source": "./src/main/js",
  "destination": "./doc",
  "plugins": [
    {"name": "esdoc2-flow-type-plugin"},
    {"name": "esdoc2-standard-plugin", "option": {
      "lint": {
        "enable": false
      }
    }},
    {
      "name": "esdoc2-coverage-plugin",
      "option": {
        "enable": true,
        "kind": ["class", "method", "member", "get", "set", "constructor", "function", "variable"]
      }
    }
  ]
}
